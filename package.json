{
	"name": "obsidian-daily-date-links",
	"version": "1.0.6",
	"description": "Obsidian plugin to create daily notes from date-formatted wikilinks.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs",
		"postversion": "git push --follow-tags",
		"setup": "./install-local.sh",
		"lint": "eslint ."
	},
	"keywords": [
		"obsidian",
		"plugin",
		"daily-notes",
		"dates"
	],
	"author": "Marco Laux",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"obsidian-daily-notes-interface": "^0.9.4"
	}
}